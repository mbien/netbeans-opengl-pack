<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>com.sun.gluegen</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>net.java.nativelibsupport</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>0.5</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.modules</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>7.3.1</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.util</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>7.10.1.1</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>com.sun.gluegen.runtime</package>
                <package>com.sun.gluegen.runtime.opengl</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/gluegen-rt.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/gluegen-rt.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
